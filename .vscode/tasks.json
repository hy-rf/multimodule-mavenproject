{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Maven Clean Compile",
      "type": "shell",
      "command": "./mvnw clean compile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$javac"],
      "dependsOn": [
        "Run db"
      ]
    },
    {
      "label": "Run db",
      "type": "shell",
      "command": "~/mmdb_store_run.sh",
      "group": {
        "kind": "none",
        "isDefault": true
      }
    }
  ]
}
